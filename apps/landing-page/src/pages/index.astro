        background-image: url("../assets/stay-tuned-bg.jpg");
        background-repeat: no-repeat;
        background-position: center;
        background-size: cover;
      }

      body::after {
        content: "";
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: rgba(0, 0, 0, 0.35);
        z-index: -1;
      }

      ol {
        counter-reset: top-ten;
      }

      ol li::before {
        content: counter(top-ten, decimal-leading-zero);
        counter-increment: top-ten;
        display: inline-block;
        margin-right: 0.5em;
        width: 2ex;
        color: rgb(254 243 199 / 0.85);
        font-size: 1.35rem;
        font-weight: 500;
      }

      @media only screen and (min-width: 1024px) {
        ol li::before {
          content: "#";
        }

        ol:hover > li::before {
          content: counter(top-ten, decimal-leading-zero);
          counter-increment: top-ten;
        }

        ol > li:hover::before {
          color: rgb(20 184 166);
        }
      }
    </style>
